{
  "name": "dotsquares-support/craft-facebook",
  "description": "A plugin for Craft CMS that helps you to add Facebook Feed in  your Craftcms website.",
  "type": "craft-plugin",
  "version": "1.1.0",
  "keywords": ["craft",
  	"cms",
  	"craftcms",
      	"facebook",
        "facebook-feed"],
  "license": "MIT",
  "authors": [
  	{
          "name": "Dotsquares",
          "homepage": "https://dotsquares.com/"
        }
  ],
  "support": {
    "email": "support.extensions@dotsquares.com",
    "issues": "https://github.com/dotsquares-support/Facebook-Feed/issues",
    "source": "https://github.com/dotsquares-support/Facebook-Feed/",
    "docs": "https://github.com/dotsquares-support/Facebook-Feed/blob/master/README.md"
  },
  "require": {
    "craftcms/cms": "^3.5.0"
  },
  "autoload": {
    "psr-4": {
      "ds\\\facebook\\": "src/"
    }
  },
  "extra": {
    "name": "Facebook Feed",
    "handle": "dsfacebook",
    "description": "Add your public facebook page feed to your Craft CMS site",
    "hasCpSection": false,
    "hasSettings": true,
    "class": "ds\\facebook\\Facebook"
  }
}
